package Venueresources;

public class Show 
{
	String showname;
	int timeofshow;
	int endofshow;
	int numberofseatsavailable;
	
	Show(String newshowname,int newtimeofshow, int newendofshow)
	{
		
		showname = newshowname;
		timeofshow = newtimeofshow;
		endofshow=newendofshow;
		numberofseatsavailable=50;
	}
	//getters
	public String getshowname()
	{
		return this.showname;
	}
	public int getshowtime()
	{
		
		return this.timeofshow;
	}
	//setters
	public void setshowtime(int newshowtime)
	{
		
		this.timeofshow=newshowtime;
	}
	public void setname(String showname)
	{
		
		this.showname=showname;
	}
	//equalsmethod
	public boolean equals(Show show1)
	{
		if(show1.showname==show2.showname && show1.timeofshow==show2.timeofshow)
		{
			return true;
		}
		return false;  
		
	}

}
