package Venueresources;

public class theater 
{
	int theaterxdimension;
	int theaterydimension;
	Seats[][] Seating = new Seats[theaterxdimension][theaterxdimension];
	
	
	theater(int newxdimension, int newydimension)
	{
		theaterxdimension = newxdimension;
		theaterydimension = newydimension;
	}
}
