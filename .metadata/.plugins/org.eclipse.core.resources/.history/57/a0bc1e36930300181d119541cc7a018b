package Venueresources;

public class Show 
{
	String name;
	int timeofshow;
	int endofshow;
	int numberofseatsavailable;
	
	Show(String newshowname,int newtimeofshow)
	{
		
		name = newshowname;
		timeofshow = newtimeofshow;
		numberofseatsavailable=50;
	}
	//getters
	public String getshowname()
	{
		return this.name;
	}
	public int getshowtime()
	{
		
		return this.timeofshow;
	}
	//setters
	public void setshowtime(int newshowtime)
	{
		
		this.timeofshow=newshowtime;
	}
	public void setname(String showname)
	{
		
		this.name=showname;
	}
	//equalsmethod
	public boolean equals(Show show1, Show show2)
	{
		if(show1.name==show2.name && show1.timeofshow==show2.timeofshow)
		{
			return true;
		}
		return false;  
		
	}

}
